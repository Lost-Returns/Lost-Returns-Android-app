<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF" /> <!-- 테두리 내부 색상 -->
            <stroke
                android:width="2dp"
                android:color="#000000" /> <!-- 테두리 두께, 색상 -->
            <corners android:radius="8dp" /> <!-- 테두리 모서리 둥글기 -->
        </shape>
    </item>
</layer-list>
